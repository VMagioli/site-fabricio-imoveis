{
    "installCommand": "npm install --legacy-peer-deps",
    "rewrites": [
        {
            "source": "/imovel/:slug",
            "has": [
                {
                    "type": "header",
                    "key": "user-agent",
                    "value": "(?i).*(facebookexternalhit|whatsapp|twitterbot|linkedinbot|telegrambot|pinterest|skype).*$"
                }
            ],
            "destination": "/api/og?slug=:slug"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}